<window id="checklistVerDialog" title="Select the sets to incorporate in the specification" border="normal" width="560px">
    <vlayout>
        <div><checkbox id="viresp" label="Always Immediate Response"/></div>
        <div><checkbox id="viprec" label="Always Immediate Precedence"/></div>
        <div><checkbox id="veiresp" label="Exist Immediate Response"/></div>
        <div><checkbox id="veresp" label="Exist Response"/></div>
        <div><checkbox id="vconf" label="Always Conflict"/></div>
        <div><checkbox id="vpar" label="Always Parallel"/></div>
        <div>
            <button id="acceptBtn" label="Accept"/>
            <button id="cancelBtn" label="Cancel"/>
        </div>
    </vlayout>
</window>