<window id="casesWindow" width="500px" sizable="true" maximizable="true" closable="true" position="center" contentStyle="overflow:auto">
    <listbox id="casesList" vflex="1" rows="10" >
        <listhead>
            <listheader id="pos" label="" sort="auto"/>
            <listheader id="case_value" label="Case ID" sort="auto"/>
            <listheader id="case_length" label="Case Length" sort="auto"/>
            <listheader id="variant_value" label="Unique Case" sort="auto"/>
			<listheader id="variant_freq" label="Percentage" sort="auto"/>
        </listhead>
    </listbox>
    <button id="save" label="Save" />
</window>
