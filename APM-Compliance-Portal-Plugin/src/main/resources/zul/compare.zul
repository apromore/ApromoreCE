<window id="win" width="300px" closable="false" position="center" title="Input specification or XML file">
    <vbox>
        <groupbox width="285px">
            <label value="Direct input:" width="100%"/>
            <textbox rows="5" cols="40" id="specificationTB">
            </textbox>
            <label value="OR Specification file(.xml):" width="100%"/>
            <hbox align="center">
                <button id="specificationUpload" label="Upload" upload="true,maxsize=-1,multiple=false,native"/>
                <label id="fileName" value="File Name" width="100%"/>
            </hbox>
        </groupbox>
        <div align="center">
            <button id="oKButton" label="OK" iconSclass="z-icon-check-circle"></button>
            <button id="cancelButton" label="Cancel" iconSclass="z-icon-times-circle"></button>
        </div>
    </vbox>
</window>
