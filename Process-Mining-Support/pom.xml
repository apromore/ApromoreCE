    <artifactId>process-mining-support</artifactId>
    <packaging>bundle</packaging>
    <name>Process Mining Support</name>
    <version>1.0</version>    
   

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <version>3.5.0</version> <!-- Note: 2.3.7 raises an ArrayIndexOutOfBound error, invalid class GrubbTest.class -->
                <extensions>true</extensions>
                <configuration>
                    <instructions>
                        <Bundle-ClassPath>.,{maven-dependencies}</Bundle-ClassPath>
                        <Import-Bundle>
                            org.apromore.prom6-osgi;bundle-version="[1.0,2.0)"
                        </Import-Bundle>
                        <Import-Package>
                            <!--
                            !junit.framework.*,
                            !org.junit.*,
                            !org.omg.spec.bpmn._20100524.model.*,
                            ee.ut.comptech.*; version="[1.0,2.0)",
                            -->
                            org.processmining.contexts.*;version="[1.0,2.0)",
                            org.processmining.framework.*;version="[1.0,2.0)",
                            org.processmining.models.*;version="[1.0,2.0)",
                            org.processmining.plugins.*;version="[1.0,2.0)",
                            *
                        </Import-Package>
                        <Export-Package>
                            com.raffaeleconforti.automaton.*,
                            com.raffaeleconforti.bpmnminer.*,
                            com.raffaeleconforti.conversion.*,
                            com.raffaeleconforti.context.*,
                            com.raffaeleconforti.foreignkeydiscovery.*,
                            com.raffaeleconforti.ilpsolverwrapper.*,
                            com.raffaeleconforti.log.util,
                            com.raffaeleconforti.noisefiltering.event.*,
                            com.raffaeleconforti.wrappers.settings.*,
                            com.raffaeleconforti.statistics.*,
                        </Export-Package>
                    </instructions>
                </configuration>
            </plugin>
        </plugins>
    </build>

