<window id="modalDialog" title="Verification" border="normal" width="550px">
    <vlayout>
        <grid id="gridMD" sclass="ap-grid ap-form">
            <columns>
                <column label="id" width="50px"/>
                <column label="Specification" width="500px"/>
            </columns>
            <rows></rows>
        </grid>
        <div sclass="ap-window-footer-actions">
            <div align="right">
                <button id="closeBtn" label="close"/>
            </div>
        </div>
    </vlayout>
</window>